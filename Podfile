# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'AQIDemo' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for AQIDemo
  pod 'Alamofire', '~> 4.5.0' 
  pod 'SwiftyJSON'
  pod 'pop'
end

target 'AQIWidget' do
    use_frameworks!
    
    # Pods for AQIDemo
    pod 'Alamofire', '~> 4.5.0'
    pod 'SwiftyJSON'
end

target 'API' do
    use_frameworks!
    
    pod 'Alamofire', '~> 4.5.0'
    pod 'SwiftyJSON'
end

target 'BluedNet' do
    use_frameworks!
    
    pod 'Alamofire', '~> 4.5.0'
    pod 'SwiftyJSON'
end

#  或者不写target，全部应用
